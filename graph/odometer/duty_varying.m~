% Graph for varying the duty cycle. each data point is the tacho count at 
% time interval of 400ms

data = [
[0 0 0 1 1 2 3 4 5]
[0 0 1 3 5 7 9 12 14]
[0 0 1 3 7 11 15 18 23]
[0 0 1 5 9 14 19 25 31]
[0 0 3 7 13 19 25 32 39]
[0 0 3 8 15 23 31 40 49]
[0 0 2 9 17 27 36 46 58]
[0 0 3 10 19 29 41 53 66]
[0 0 3 10 21 32 46 60 77]];

duty = [10,20,30,40,50,60,70,80,90];
dist = [1,3,7,10,14,17,21,24.5,28.5];

[~,n] = size(dist);
labels = cell(n,1);
XTickLabels = cell(n,1);
for i=1:n
    labels{i} = sprintf('duty %d, dist %.1f', duty(i), dist(i));
    XTickLabels{i} = sprintf('%d', 400*i);
end

plot (data')
title('Tacho counts increases linearly as duty cycle increases')
xlabel('Time (ms)')
set(gca, 'Xtick', 1
ylabel('Average Tacho Counts between L and R motor')
legend(labels, 'Location', 'best')