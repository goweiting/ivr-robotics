first = gyro_circuit(1)
gyro_circuit = gyro_circuit - first;

figure; hold on;
[~,n] = size(data_circuit);
ys = linspace(0,n,n);

subplot(1,2,1)
plot(data_circuit, ys, 'g');
title('Error values of color value around the circuit clockwise');
xlabel('Error from PD controller');
ylabel('Distance around the circuit');


